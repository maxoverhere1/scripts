# Contentful Configuration
# Copy this file to .env and fill in your values

# Space ID (e.g., 3p2fxa94bzao)
CONTENTFUL_SPACE_ID=

# Environment ID (e.g., master, staging, pre-prod)
CONTENTFUL_ENVIRONMENT_ID=

# Management API Token (CMA token - required for updating content)
# Get this from: Settings > API keys > Content management tokens
CONTENTFUL_MANAGEMENT_TOKEN=

