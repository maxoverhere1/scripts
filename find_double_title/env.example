# Contentful Configuration
# Copy this file to .env and fill in your values

# Space ID (e.g., 3p2fxa94bzao)
CONTENTFUL_SPACE_ID=

# Environment ID (e.g., master, staging, pre-prod)
CONTENTFUL_ENVIRONMENT_ID=

# Delivery API Token (CDA token - read-only access)
# Get this from: Settings > API keys > Content delivery / preview tokens
CONTENTFUL_DELIVERY_TOKEN=


